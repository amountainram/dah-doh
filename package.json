  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./promises": {
      "default": "./dist/promises.js",
      "import": "./dist/promises.js",
      "types": "./dist/types/promises.d.ts"
    }
  },
  "files": [
    "dist",
    "promises",
    "./package.json",
    "./LICENSE",
