    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
